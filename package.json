{
  "name": "financial_app_backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "nest start",
    "start:dev": "nest start --watch",
    "build": "nest build",
    "seed": "ts-node -r tsconfig-paths/register src/seeds/seed.ts",
    "typeorm:gen": "ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js migration:generate -d src/typeorm.config.ts src/infrastructure/persistence/typeorm/migrations/auto",
    "typeorm:run": "ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js migration:run -d src/typeorm.config.ts",
    "typeorm:revert": "ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js migration:revert -d src/typeorm.config.ts",
    "docs:openapi": "ts-node -r tsconfig-paths/register src/docs/openapi.ts",
    "test": "jest"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.0.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/typeorm": "^10.0.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pdfkit": "^0.17.1",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.11.0",
    "@types/passport-jwt": "^3.0.9",
    "@types/pdfkit": "^0.17.2",
    "jest": "^29.7.0",
    "swagger-ui-express": "^5.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.0",
    "yaml": "^2.5.0"
  }
}
